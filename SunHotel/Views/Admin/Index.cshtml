@model ViewModels.DashboardVM
@{
    ViewData["Title"] = "Admin";
    Layout = "~/Views/Shared/_Layoutnew.cshtml";


}



<br />
<br />
<br />

<div class="text-center">


    <div class="row row-cols-1 row-cols-md-3">
        <div class="col mb-4">
            <div class="card h-100">
                <div class="card-body">
                    <h5 class="card-title">Habitaciones</h5>
                    <p class="card-text">@Model.Canthabitaciones</p>
                </div>
            </div>
        </div>
        <div class="col mb-4">
            <div class="card h-100">
                <div class="card-body">
                    <h5 class="card-title">Tipos de Habitaciones</h5>
                    <p class="card-text">@Model.CantTipohabitaciones</p>
                </div>
            </div>
        </div>
        <div class="col mb-4">
            <div class="card h-100">
                <div class="card-body">
                    <h5 class="card-title">Edificios</h5>
                    <p class="card-text">@Model.Cantedificios</p>
                </div>
            </div>
        </div>
        <div class="col mb-4">
            <div class="card h-100">
                <div class="card-body">
                    <h5 class="card-title">Reservas Hoy</h5>
                    <p class="card-text">@Model.reservas</p>
                </div>
            </div>
        </div>
    </div>
</div>
